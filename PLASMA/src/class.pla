import stdlib
  predef putc, puts
end
import testcls
  word print
  const dec = 0
  const hex = 2
end

byte spaces[] = "   "

def putln
  putc($0D)
end

def printnums
  word i
  i = 10000
  repeat
    print:dec(i)
    puts(@spaces)
    print:hex(i)
    putln
    i = i / 10
  until i == 0
end
printnums
done
